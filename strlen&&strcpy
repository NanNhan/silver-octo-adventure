char* my_strcpy(char* arr1,const char* arr2)
{
	assert(arr1 != NULL);
	assert(arr2 != NULL);
	while(*arr1++ = *arr2++)
	{
		;	
	}
	return arr1;
}
int main()
{
	char arr1[] = "abcdef";
	char arr2[] = "bit";
	my_strcpy(arr1,arr2);
	printf("%s",strcpy(arr1,arr2));
	return 0;
}
